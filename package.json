{
  "name": "medusa-plugin-algolia-extended",
  "version": "0.0.13",
  "description": "Extended Algolia search plugin for Medusa",
  "author": "Tsvetan Nikolov <live.webscore@gmail.com>",
  "license": "MIT",
  "keywords": [
    "ecommerce",
    "medusa",
    "medusa-plugin-algolia-extended",
    "medusa-plugin-search"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:tsvetann/medusa-plugin-algolia-extended.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "babel src --out-dir ./dist --ignore **/__tests__ --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "start:custom": "cross-env npm run build && node --preserve-symlinks index.js",
    "dev": "cross-env npm run build && medusa develop",
    "build:admin": "cross-env medusa-admin build"
  },
  "dependencies": {
    "@medusajs/utils": "^1.9.5",
    "algoliasearch": "^4.17.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.12.0",
    "medusa-interfaces": "^1.3.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/admin": "^6.0.0",
    "@medusajs/event-bus-local": "^1.9.4",
    "@medusajs/event-bus-redis": "^1.8.7",
    "@medusajs/medusa": "^1.12.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.13",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "express": "^4.17.2",
    "jest": "^27.3.1",
    "medusa-interfaces": "^1.3.7",
    "mongoose": "^5.13.14",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typeorm": "^0.3.16",
    "typescript": "^4.5.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
